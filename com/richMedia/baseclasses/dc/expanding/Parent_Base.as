package com.richMedia.baseclasses.dc.expanding{	import flash.display.MovieClip;	import flash.events.Event;		import com.google.ads.studio.HtmlEnabler;	import com.google.ads.studio.utils.StudioClassAccessor;		import com.richMedia.dateUtils.TuneIn;    public class Parent_Base extends MovieClip    {		protected var enabler:HtmlEnabler;		protected var expanding:Object;				protected function setTunein(dData:Array):void 		{			if( !dData ) return;			var dD:Object;			TuneIn.addDefault( "default", 1 ); // default date			for( var i:uint = 0; i < dData.length; i++ )			{				dD = dData[i];				if( dD.frameLabel == "testdate" )				{					TuneIn.setTestDateByDate( dD );				}				else				{					TuneIn.addDate( dD, dD.frameLabel );				}			}			TuneIn.parseDate();		}		public function Parent_Base() 		{			if( stage ) init();			else addEventListener( Event.ADDED_TO_STAGE, init ); 								  										}				protected function init( e:Event = null ):void 		{			addEventListener( Event.REMOVED_FROM_STAGE, destroy );						enabler = HtmlEnabler.getInstance();			enabler.init(this);						expanding = StudioClassAccessor.getClass(StudioClassAccessor.CLASS_EXPANDING)["getInstance"]();									if( DCData.IS_AUTO_PANEL ) expanding.startExpanded("autoPanel");		}				protected function destroy( e:Event = null ):void 		{			removeEventListener( Event.REMOVED_FROM_STAGE, destroy );									expanding = null;						enabler = null;		}	}}